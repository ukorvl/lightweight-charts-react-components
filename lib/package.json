{
  "name": "lightweight-charts-react-components",
  "version": "0.0.2",
  "scripts": {
    "build": "rslib build"
  },
  "description": "React components for Lightweight Charts",
  "keywords": [
    "react",
    "lightweight-charts",
    "data-visualization",
    "trading",
    "charting",
    "stock",
    "market",
    "finance"
  ],
  "repository": "https://github.com/ukorvl/lightweight-charts-react-components",
  "homepage": "https://github.com/ukorvl/lightweight-charts-react-components#readme",
  "bugs": {
    "url": "https://github.com/ukorvl/lightweight-charts-react-components/issues"
  },
  "type": "module",
  "main": "./dist/lightweight-charts-react-components.cjs",
  "module": "./dist/lightweight-charts-react-components.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/lightweight-charts-react-components.standalone.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/lightweight-charts-react-components.js",
      "require": "./dist/ightweight-charts-react-components.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "ukorvl",
  "license": "MIT",
  "peerDependencies": {
    "lightweight-charts": ">=4 < 5",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "provenance": true,
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.1",
    "@rsbuild/plugin-react": "^1.1.0",
    "@rslib/core": "^0.1.4",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "rsbuild-plugin-dts": "^0.5.5",
    "vitest": "^2.1.9"
  }
}
